{
  "name": "nasa-cli",
  "version": "1.0.6",
  "description": "Download NASA Picture of the Day from your terminal!",
  "main": "cli.js",
  "bin": {
    "nasa": "cli.js"
  },
  "preferGlobal": "true",
  "dependencies": {
    "chalk": "^2.4.0",
    "cheerio": "^1.0.0-rc.2",
    "execa": "^0.10.0",
    "fs-extra": "^6.0.0",
    "got": "^8.3.0",
    "log-symbols": "^2.1.0",
    "log-update": "^2.3.0",
    "ora": "^2.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "^3.0.0",
    "nyc": "^12.0.1",
    "xo": "*"
  },
  "xo": {
    "esnext": true
  },
  "scripts": {
    "test": "xo --fix && nyc --reporter=html --reporter=text ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxczaki/nasa-cli.git"
  },
  "keywords": [
    "nasa-cli",
    "nasa",
    "space",
    "galaxy",
    "astronomy",
    "picture",
    "day",
    "picture-of-the-day",
    "cli",
    "cli-app",
    "npm",
    "xo-style",
    "nodejs"
  ],
  "author": "Antoni Kepinski <hello@akepinski.me> (https://akepinski.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xxczaki/nasa-cli/issues"
  },
  "homepage": "https://github.com/xxczaki/nasa-cli#readme"
}
