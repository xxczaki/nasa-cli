{
  "name": "nasa-cli",
  "version": "2.0.0",
  "description": "Download NASA Picture of the Day from your terminal!",
  "main": "cli.js",
  "bin": {
    "nasa": "cli.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "fs-extra": "^7.0.0",
    "got": "^11.8.5",
    "log-symbols": "^2.2.0",
    "log-update": "^2.3.0",
    "ora": "^3.0.0",
    "youtube-dl": "przemyslawpluta/node-youtube-dl#3349cbb6e067b6c9e7605acfa4f9d1ccc895fb0c"
  },
  "devDependencies": {
    "ava": "*",
    "execa": "*",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "no-useless-escape": 0,
      "no-unused-vars": 0
    }
  },
  "scripts": {
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxczaki/nasa-cli.git"
  },
  "keywords": [
    "nasa-cli",
    "nasa",
    "space",
    "galaxy",
    "astronomy",
    "picture",
    "day",
    "picture-of-the-day",
    "cli",
    "cli-app",
    "npm",
    "xo-style",
    "nodejs"
  ],
  "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xxczaki/nasa-cli/issues"
  },
  "homepage": "https://github.com/xxczaki/nasa-cli#readme"
}
